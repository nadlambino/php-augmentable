{
    "name": "inspira/augmentable",
    "description": "Augment PHP class at runtime.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inspira\\Augmentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inspira\\Augmentable\\tests\\Fixtures\\": "tests/Fixtures"
        }
    },
    "authors": [
        {
            "name": "Ronald Lambino",
            "email": "50892397+nadlambino@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "8.3"
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
